<!DOCTYPE html>
<html lang="en">

<head>
  <script src="//cdn.jsdelivr.net/npm/phaser@3.20.1/dist/phaser.min.js"></script>
</head>

<body>
  <script>
    var config = {
      type: Phaser.AUTO,
      width: 1200,
      height: 960,
      parent: 'game',
      scene: {
        preload: preload,
        create: create
      },
      physics: {
        default: "matter",
        matter: {
          // debug: true
        }
      }
    };

    var game = new Phaser.Game(config);

    function preload() {
      this.load.atlas('sheet', '../images/phaser/physical/shengdanyounian.png', '../images/phaser/physical/test.json');
      this.load.json('shapes', '../images/phaser/physical/test.json');
    }

    function create() {

      var shapes = this.cache.json.get('shapes')

      this.matter.world.setBounds(0, 0, game.config.width, game.config.height);

      console.log('shapes.eye->',shapes.eye)

      this.matter.add.sprite(200, 50, 'sheet', 'eye', { shape: shapes.eye });
      this.matter.add.sprite(250, 250, 'sheet', 'nose', { shape: shapes.nose });
      this.matter.add.sprite(360, 50, 'sheet', 'mouth', { shape: shapes.mouth });
      this.matter.add.sprite(400, 250, 'sheet', 'hat', { shape: shapes.hat });
    }
  </script>
</body>


</html>